<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">aiti_productresponsibleuseradminui_responsible_listing.aiti_productresponsibleuseradminui_responsible_listing_data_source</item>
        </item>
    </argument>
    <settings>
        <spinner>aiti_productresponsibleuseradminui_responsible_columns</spinner>
        <deps>
            <dep>aiti_productresponsibleuseradminui_responsible_listing.aiti_productresponsibleuseradminui_responsible_listing_data_source</dep>
        </deps>
    </settings>
    <dataSource name="aiti_productresponsibleuseradminui_responsible_listing_data_source" component="Magento_Ui/js/grid/provider">
        <settings>
            <storageConfig>
                <param name="indexField" xsi:type="string">responsible_id</param>
            </storageConfig>
            <updateUrl path="mui/index/render"/>
        </settings>
        <aclResource>Aiti_ProductResponsibleUserAdminUi::responsible</aclResource>
        <dataProvider name="aiti_productresponsibleuseradminui_responsible_listing_data_source" class="Aiti\ProductResponsibleUserAdminUi\Ui\DataProvider\ProductResponsibleUser\ListingDataProvider">
            <settings>
                <requestFieldName>id</requestFieldName>
                <primaryFieldName>user_id</primaryFieldName>
            </settings>
        </dataProvider>
    </dataSource>
    <listingToolbar name="listing_top">
        <settings>
            <sticky>true</sticky>
        </settings>
        <bookmark name="bookmarks"/>
        <columnsControls name="columns_controls"/>
        <filters name="listing_filters"/>
        <paging name="listing_paging"/>
    </listingToolbar>
    <columns name="aiti_productresponsibleuseradminui_responsible_columns">
        <selectionsColumn name="ids">
            <settings>
                <indexField>responsible_id</indexField>
            </settings>
        </selectionsColumn>
        <column name="responsible_id">
            <settings>
                <filter>text</filter>
                <sorting>asc</sorting>
                <label translate="true">ID</label>
            </settings>
        </column>
        <column name="user_id">
            <settings>
                <filter>text</filter>
                <label translate="true">user_id</label>
            </settings>
        </column>
    </columns>
</listing>
